    "next": "^15.3.5",
    "next-seo": "^6.8.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-tsparticles": "^2.12.2",
    "swiper": "^11.2.10",
    "tsparticles": "^3.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
